div
  font-family ChunkFive
body
  width 100%
  margin 0 auto
  background-color rgb(230,230,240)
.um-full
  width 100%
  z-index -100px
#um-header-title
  position absolute
  background-image url(http://www.ultimatemods.net:8888/images/blueprint-crop.jpg)
  background-repeat round
  left 0px
  right 0px
  top 0px
  border-width 0px
  height 300px
  width 100%
#um-header-title-img
  max-height 250px
  width 70%
  display block
  margin 50px auto
#um-body
  position absolute
  top 300px
  left 0px
  right 0px
  z-index 100px
  width 100%
footer
  width 100%
  position absolute
  bottom 0px
  left 0px
  border 0px solid black
  font-family ChunkFive
#um-footer-img
  background-image url(http://www.ultimatemods.net:8888/images/blueprint-crop.jpg)
  background-repeat round
  border 0px solid black
  width 100%
  height 300px
  position absolute
.footer-label a
  font-size 3em
  top 100px
  text-align center
  width 100%
  left 1px
  position absolute
  color white
  text-decoration none
  list-style-type none
  font-style bold
  font-family "Exo"
#social
  top 170px
  text-align center
  width 100%
  left 1px
  position absolute
  text-decoration none
  list-style-type none
